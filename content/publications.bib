  keyword      = {Ontology engineering,HEALTH-CARE,co-design,continuous care,eHealth,participatory,IBCN},
  language     = {eng},
  number       = {1},
  pages        = {27--64},
  title        = {An ontology co-design method for the co-creation of a continuous care ontology},
  url          = {http://dx.doi.org/10.3233/AO-140131},
  volume       = {9},
  year         = {2014},
}

