@mastersthesis{ongenae_mastersthesis_2007,
  author = {Ongenae, Femke},
  title = {Study and simulation of a person oriented nurse call system},
  school = {Ghent University},
  year = 2007,
  month = Jun,
  abstract = {The current nurse call systems are organised according to rooms:  each room (or bed) has a button that can be used by the patients to call a nurse. In the future, there will be an evolution to a button for each patient so that they can walk around freely and still make calls. This signifies the transition from a place oriented to a person oriented system. In this thesis a person oriented nurse call system is described.  An ontology was designed that holds the necessary context information.  A nurse call algorithm that uses this information was constructed. The IBCN Context framework was used to implement the whole. Rules are formulated to implement the algorithm. OWL and Jena were applied to integrate the ontology in the framework. A couple of Web services were set up. They allow us to insert new information into the knowledge base or extract information from it.  At last a simulation was set up to show the advantages of the personoriented approach},
  url = {https://lib.ugent.be/catalog/rug01:001312120},
  keywords = {nurse call system, person oriented, context, ontology, OWL, OSGi},
}